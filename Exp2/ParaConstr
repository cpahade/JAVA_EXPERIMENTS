package JavaExperiments;
class Cars {
	String model;
	int year;
	Cars(String m , int y)
	{
		model = m;
		year = y;
	}
void display() 
	{
		System.out.println("Car Model: " + model + "Year: " +year);
	}
}

	
public class ParaConstr {

	public static void main(String[] args) 
	{
		Cars Mycar =new Car("TOYOTA ", 2023);
		Mycar.display();
	}
}
